					"v.ground_speed=q.ground_speed;",
					"v.mod_move_speed=query.modified_move_speed;",
					"v.tornado = q.variant == 1 && v.tornado_start != 1 ? 360 : v.tornado;",
					"v.tornado = v.tornado_start ? v.tornado + 900*query.delta_time : v.tornado;",
					"v.tornado = v.tornado > 360 ? 0 : v.tornado;",
					"v.direction_rot = math.mod(math.atan2(q.movement_direction(0), q.movement_direction(2)) + q.head_y_rotation(0), 360);",
					"v.direction = (v.direction_rot > -360 && v.direction_rot < -270) || (v.direction_rot > -90 && v.direction_rot < 90) || (v.direction_rot > 181)? 1 : -1;",
					"t.air_blade_rot = q.mark_variant == 33 ? 0 : 0;",
					"v.air_blade_rot = q.mark_variant == 33 && q.variant == 2 ? 0.33 : v.air_blade_rot;",
					"v.air_blade_rot = q.mark_variant == 33 && q.variant == 3 ? -0.33 : v.air_blade_rot;",
					"v.air_blade_rot = q.mark_variant == 33 && q.variant == 4 ? 1 : v.air_blade_rot;",
					"t.air_blade_rot = q.mark_variant == 33 && q.variant >= 2 ? v.air_blade_rot : t.air_blade_rot;",
