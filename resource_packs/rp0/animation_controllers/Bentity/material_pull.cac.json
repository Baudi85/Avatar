{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.material_pull.particle_cnt": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"play_earth_particle": "q.life_time>=0.4&&q.variant==0"
						},
						{
							"play_water_particle": "q.life_time>=0.4&&q.variant==1"
						}
					]
				},
				"play_earth_particle": {
					"animations": [
						{
							"earth_particle": "q.life_time<=2.5"
						}
					]
				},
				"play_water_particle": {
					"animations": [
						{
							"water_particle": "q.life_time<=2.5"
						}
					]
				}
			}
		}
	}
}