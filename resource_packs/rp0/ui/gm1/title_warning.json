        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "/warning",
                "source_property_name": "(#value and not (#value = ''))",
                "target_property_name": "#visible"
            }
        ]
    },
    "orb_column": { "type": "panel", "size": [ "2%", "100%" ], "$w": 0, "controls": [{ "orb@orb": {}}]},
    "orb": {
        "anchor_from": "bottom_middle",
        "anchor_to": "center",
        "type": "image",
        "size": [ 24, 24 ],
        "texture": "textures/particle/light_orb",
        "$rise": "('gm1_title_warning.anim__rise__' + $w)",
        "$shrink": "('gm1_title_warning.anim__shrink__' + $w)",
        "$color": "('gm1_title_warning.anim__color__' + $w)",
        "anims": [ "@$rise", "@$shrink", "@$color" ]
    },
    "anim__rise": {
        "anim_type": "offset",
        "from": [ 0, 0 ],
        "to": [ 0, "-50%" ],
        "duration": 1.5,
        "next": "@gm1_title_warning.anim__rise"
    },
    "anim__shrink": {
        "anim_type": "size",
        "from": [ 24, 24 ],
        "to": [ 0, 0 ],
        "easing": "out_sine",
        "duration": 1.5,
        "next": "@gm1_title_warning.anim__shrink"
    },
    "anim__color": {
        "anim_type": "color",
        "from": [ 0.898, 0.4, 0.4, 1 ],
        "to": [ 1, 1, 0.929, 0.98 ],
        "duration": 1.5,
        "next": "@gm1_title_warning.anim__color"
    },
    "anim__rise__0": { "anim_type": "wait", "next": "@gm1_title_warning.anim__rise", "duration": 0 },
    "anim__shrink__0": { "anim_type": "wait", "next": "@gm1_title_warning.anim__shrink", "duration": 0 },
