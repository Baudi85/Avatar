{
	"format_version": "1.10.0",
	"render_controllers": {
		"controller.render.trainer": {
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "material.default"
				}
			],
			"textures": [
				"texture.default"
			],
			"overlay_color": {
				"a": "v.distance_from_camera<=5?0:0.9",
				"r": 0.1,
				"g": 0.1,
				"b": 0.1
			},
			"ignore_lighting": true,
			"light_color_multiplier": "v.distance_from_camera <= 5 ? 1 : 0.5"
		}
	}
}