{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.d51e3104ef76f6042f3b4f1f024a335295b7be3e_0": {
			"states": {
				"default": {
					"on_entry": [
						"/tag @s add can_end_state"
					],
					"on_exit": [
						"/tag @s remove can_end_state"
					],
					"transitions": [
						{
							"not_in_state": "!(1&&query.skin_id==100)"
						}
					]
				},
				"not_in_state": {
					"transitions": [
						{
							"default": "1&&query.skin_id==100"
						}
					]
				}
			},
			"initial_state": "not_in_state"
		},
		"controller.animation.d51e3104ef76f6042f3b4f1f024a335295b7be3e_1": {
			"states": {
				"default": {
					"on_entry": [
						"@s nom:state_bending_teq_0",
						"@s nom:enemy.bending.earth.pull.161901.move",
						"@s nom:enemy.bending.earth.pull.161901.shooter",
						"v.runtime=q.life_time;"
					],
					"transitions": [
						{
							"not_in_state": "!(1&&query.skin_id==100)"
						},
						{
							"charged": "query.is_charged&&query.is_saddled&&query.ground_speed<0.25"
						}
					]
				},
				"charged": {
					"on_entry": [
						"@s nom:state_bending_teq_1"
					],
					"transitions": [
						{
							"not_in_state": "!(1&&query.skin_id==100)"
						},
						{
							"default": "!query.is_saddled"
						},
						{
							"attack": "!query.is_charged"
						}
					]
				},
				"attack": {
					"on_entry": [
						"v.runtime=q.life_time;",
						"@s nom:state_bending_teq_2"
					],
					"on_exit": [
						"@s nom:state_bending_teq_0",
						"@s nom:enemy.bending.earth.pull.161901.stop"
					],
					"transitions": [
						{
							"not_in_state": "!(1&&query.skin_id==100)"
						},
						{
							"cooldown": "q.life_time>=v.runtime"
						}
					]
				},
				"cooldown": {
					"on_entry": [
						"v.runtime=q.life_time;"
					],
					"on_exit": [
						"@s nom:enemy.bending.earth.pull.161901.end",
						"/event entity @s[tag=can_end_state] gm1:end_state"
					],
					"transitions": [
						{
							"not_in_state": "!(1&&query.skin_id==100)"
						},
						{
							"end": "q.life_time>=v.runtime+0.75"
						}
					]
				},
				"end": {
					"transitions": [
						{
							"not_in_state": "!(1&&query.skin_id==100)"
						}
					]
				},
				"not_in_state": {
					"transitions": [
						{
							"default": "1&&query.skin_id==100"
						}
					]
				}
			},
			"initial_state": "not_in_state"
		}
	}
}